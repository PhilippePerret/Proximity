{
  "name": "proximity",
  "productName": "Proximity",
  "version": "1.2.3",
  "description": "Application de bureau pour gérer les proximités de mots dans un texte.",
  "main": "app.js",
  "scripts": {
    "tests": "TESTS=true electron .",
    "start": "electron .",
    "start-update": "UPDATE_MAIN_HTML=true electron .",
    "build": "electron-packager . --platform=darwin --asar --out=built --overwrite",
