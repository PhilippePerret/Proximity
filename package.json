  "devDependencies": {
    "electron": "^6.1.5",
    "electron-packager": "^14.0.4",
    "spectron": "^8.0.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "adm-zip": "^0.4.13",
    "ava": "^3.1.0",
    "electron-log": "^3.0.8",
    "events": "^3.0.0",
    "glob": "^7.1.4",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "nlp-js-tools-french": "^1.0.9",
    "rimraf": "^3.0.0",
    "treetagger": "^0.1.1"
  }
